    <!-- http://purl.org/linked-data/cube# -->


    <owl:Class rdf:about="http://purl.org/linked-data/cube#"/>
    


    <!-- https://w3id.org/sri#Building -->


    <owl:Class rdf:about="https://w3id.org/sri#Building">
        <rdfs:comment xml:lang="en">This represents a building which is assessed.</rdfs:comment>
        <rdfs:label xml:lang="en">Building</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sri#Domain -->


    <owl:Class rdf:about="https://w3id.org/sri#Domain">
        <rdfs:subClassOf rdf:resource="https://w3id.org/sri#FunctionalThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#narrower"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/sri#Service"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://w3id.org/sri#Service"/>
        <rdfs:comment xml:lang="en">Class of technical domains.</rdfs:comment>
        <rdfs:label xml:lang="en">Domain</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sri#FunctionalThing -->


    <owl:Class rdf:about="https://w3id.org/sri#FunctionalThing">
        <rdfs:comment xml:lang="en">Class of instances in the functional hierarchy, this includes Domains and Services.</rdfs:comment>
        <rdfs:label xml:lang="en">FunctionalThing</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sri#Impact -->


    <owl:Class rdf:about="https://w3id.org/sri#Impact">
        <rdfs:subClassOf rdf:resource="https://w3id.org/sri#ImpactThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#broader"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/sri#KeyCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://w3id.org/sri#KeyCapability"/>
        <rdfs:comment xml:lang="en">Class of Impacts.</rdfs:comment>
        <rdfs:label xml:lang="en">Impact</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sri#ImpactThing -->


    <owl:Class rdf:about="https://w3id.org/sri#ImpactThing">
        <rdfs:comment xml:lang="en">Class of all instances in the impact hierarchy, this includes Impacts and Key Capabilities.</rdfs:comment>
        <rdfs:label xml:lang="en">ImpactThing</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sri#KeyCapability -->


    <owl:Class rdf:about="https://w3id.org/sri#KeyCapability">
        <rdfs:subClassOf rdf:resource="https://w3id.org/sri#ImpactThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#narrower"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/sri#Impact"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Class of all Key Capabilities, aggregation of Impacts.</rdfs:comment>
        <rdfs:label xml:lang="en">KeyCapability</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sri#Level -->


    <owl:Class rdf:about="https://w3id.org/sri#Level">
        <rdfs:comment xml:lang="en">Class of service levels.</rdfs:comment>
        <rdfs:label xml:lang="en">Level</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sri#Service -->


    <owl:Class rdf:about="https://w3id.org/sri#Service">
        <rdfs:subClassOf rdf:resource="https://w3id.org/sri#FunctionalThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#broader"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/sri#Domain"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Class for Services.</rdfs:comment>
        <rdfs:label xml:lang="en">Service</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sri#ServiceCatalogue -->


    <owl:Class rdf:about="https://w3id.org/sri#ServiceCatalogue">
        <rdfs:comment xml:lang="en">A Service Catalogue is a collection of services used in an assessment.</rdfs:comment>
        <rdfs:label xml:lang="en">Service catalogue</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sri#ServiceLevel -->


    <owl:Class rdf:about="https://w3id.org/sri#ServiceLevel">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/sri#function"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/sri#Service"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/sri#level"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/sri#Level"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Class for all ServiceLevel instances, which are all allowed/defined combinations of Services and Levels.</rdfs:comment>
        <rdfs:label xml:lang="en">Service level</rdfs:label>
    </owl:Class>
    


